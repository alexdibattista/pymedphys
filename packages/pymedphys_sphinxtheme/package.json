{
  "name": "pymedphys_sphinxtheme",
  "main": "js/theme.js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "webpack && yarn install:python",
    "install:python": "python -c \"import os; os.system('pip install .') if 'NODE_ENV' in os.environ and os.environ['NODE_ENV'] == 'production' else os.system('pip install -e .')\"",
    "build": "python setup.py sdist bdist_wheel",
    "upload": "twine upload dist/*",
    "watch": "yarn webpack --mode=development --watch"
  },
  "dependencies": {
    "bourbon": "~4.3",
    "bourbon-neat": "~1.7",
    "browserify": "^13.0.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "lato-font": "^3.0.0",
    "matchdep": "~1.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "roboto-fontface": "^0.10.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "wyrm": "^1.0.9"
  }
}