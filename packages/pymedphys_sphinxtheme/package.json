{
  "name": "pymedphys_sphinxtheme",
  "main": "js/theme.js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "webpack && yarn install:python",
    "install:python": "python -c \"import os; os.system('pip install .') if 'NODE_ENV' in os.environ and os.environ['NODE_ENV'] == 'production' else os.system('pip install -e .')\"",
    "build": "python setup.py sdist bdist_wheel",
    "upload": "twine upload dist/*"
  },
  "dependencies": {
    "bourbon": "~4.3",
    "bourbon-neat": "~1.7",
    "browserify": "^13.0.0",
    "font-awesome": "~4.7",
    "grunt": "~1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-copy": "~1.0.0",
    "grunt-contrib-sass": "~1.0.0",
    "jquery": "^3.4.1",
    "matchdep": "~1.0.1",
    "wyrm": "^1.0.9"
  },
  "devDependencies": {
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  }
}